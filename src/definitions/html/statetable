ANAME		save	ANAME
ANAME	=	aname	AVAL
ANAME	>	aname;adup;stagc	PCDATA
ANAME	EOF	aname;adup;stagc	DONE
ANAME	S	aname	EQ
APOS		save	APOS
APOS	&	save;push	ENT
APOS	'	aval	TAGWS
APOS	EOF	aval;stagc	DONE
APOS	S	sp	APOS
AVAL		save	STAGC
AVAL	"	skip	QUOT
AVAL	'	skip	APOS
AVAL	>	aval;stagc	PCDATA
AVAL	EOF	aval;stagc	DONE
AVAL	S	skip	AVAL
CDATA		save	CDATA
CDATA	<	save	CDATA2
CDATA	CR	lf	CCRLF
CDATA	EOF	pcdata	DONE
CDATA2		save	CDATA
CDATA2	/	unsave;pcdata	ETAG
CDATA2	EOF	unsave;pcdata	DONE
COM		skip	COM
COM	-	skip	COMD
COM	EOF	skip	DONE
COMD		skip	COM
COMD	-	skip	COMD
COMD	>	skip	PCDATA
COMD	EOF	skip	DONE
CRLF		unget	PCDATA
CRLF	LF	skip	PCDATA
CRLF	EOF	skip	DONE
CCRLF		unget	CDATA
CCRLF	LF	skip	CDATA
CCRLF	EOF	skip	DONE
DECL		skip	DECL2
DECL	-	skip	COM
DECL	>	skip	PCDATA
DECL	EOF	skip	DONE
DECL2		skip	DECL2
DECL2	>	skip	PCDATA
DECL2	EOF	skip	DONE
ENT		entity	ENT
ENT	EOF	entity	DONE
EQ		adup;save	ANAME
EQ	=	skip	AVAL
EQ	>	adup	PCDATA
EQ	EOF	adup;stagc	DONE
EQ	S	skip	EQ
ETAG		save	ETAG
ETAG	>	etag	PCDATA
ETAG	EOF	etag	DONE
ETAG	S	skip	ETAG
GI		save	GI
GI	/	skip	GI
GI	>	gi;stagc	PCDATA
GI	EOF	skip	DONE
GI	S	gi	TAGWS
PCDATA		save	PCDATA
PCDATA	&	pcdata;save;push	ENT
PCDATA	<	pcdata	TAG
PCDATA	CR	lf	CRLF
PCDATA	EOF	pcdata	DONE
PCDATA	S	sp	PCDATA
PI		save	PI
PI	>	pi	PCDATA
PI	EOF	pi	DONE
PITARGET		save	PITARGET
PITARGET	>	pitarget;pi	PCDATA
PITARGET	EOF	pitarget;pi	DONE
PITARGET	S	pitarget	PI
QUOT		save	QUOT
QUOT	"	aval	TAGWS
QUOT	&	save;push	ENT
QUOT	EOF	aval;stagc	DONE
QUOT	S	sp	QUOT
STAGC		save	STAGC
STAGC	>	aval;stagc	PCDATA
STAGC	EOF	aval;stagc	DONE
STAGC	S	aval	TAGWS
TAG		save	GI
TAG	!	skip	DECL
TAG	/	skip	ETAG
TAG	?	skip	PITARGET
TAG	EOF	lt;pcdata	DONE
TAG	S	lt	PCDATA
TAGWS		save	ANAME
TAGWS	/	skip	TAGWS
TAGWS	>	stagc	PCDATA
TAGWS	EOF	stagc	DONE
TAGWS	S	skip	TAGWS
